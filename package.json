    "prettier": "1.11.1",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  }
}
