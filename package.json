{
  "name": "Boilerplate",
  "version": "0.1.0",
  "description": "Our boilerplate for a fullstack web app using React/Redux + Sequelize/Postgres",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-optimal": "NODE_ENV='production' npm run build-client & NODE_ENV='production' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "author": "Chiara Marcial Martínez e Iván Felix Herrera",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/translate": "^2.1.2",
    "axios": "^0.15.3",
    "bootstrap": "^4.1.3",
    "bootstrap-social": "^5.1.1",
    "chalk": "^2.4.1",
    "chalk-animation": "^1.6.0",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.1",
    "history": "^4.6.3",
    "localized-strings": "^0.2.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-burger-menu": "^2.5.4",
    "react-contenteditable": "^3.1.1",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.3.1",
    "redux": "^3.6.0",
    "redux-burger-menu": "^0.2.8",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.38.0",
    "volleyball": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.54",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-function-sent": "7.0.0-beta.54",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.54",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-beta.54",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/register": "^7.0.0-beta.55",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "babel-preset-minify": "^0.5.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^1.0.0",
    "cssnano": "^4.1.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
